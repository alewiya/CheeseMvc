<h1>@ViewBag.title</h1>
<form method="post">
    @foreach(var cheese in ViewBag.Cheeses)
    {
        <input type="checkbox" name="cheeseIds" id="cheese.CheeseId" value="@cheese.Name" />
        <label for="@cheese.CheeseId">@cheese.Name</label>
        <br />
    }
    <input type="submit" value="Remove Cheeses" />
</form>

