@model List<Cheese>
<h1>@ViewBag.title</h1>

@if (Model.Count == 0)
{
    <p>No cheeses yet!</p>
}

<table class="table">

    <tr>
        <th>Name</th>
        <th>Description</th>
        <th>Edit</th>
        <th>Type</th>
        <th>Rating</th>
    </tr>

    @foreach (Cheese cheese in Model)
    {
<tr>
    <td>@cheese.Name</td>
    <td>@cheese.Description</td>

    <td>
        <a asp-controller="Cheese" asp-action="Edit" asp-route-cheeseId="@cheese.CheeseId">edit</a>
    </td>
    <td>@cheese.Type</td>
    <td>@cheese.Rating </td>
</tr>
    }
</table>


